package has_a_relation;
import java.util.Scanner;
class Dish
{
	private String dName;
	private double price;
	//getter
	String getDname()
	{
		return dName;
	}
	double getPrice()
	{
		return price;
	}
	//setters
	void setDname(String dName)
	{
		this.dName=dName;
	}
	void setPrice(double price)
	{
		this.price=price;
	}
	//CONSTRUCTORS
	Dish(String dName)
	{
		setDname(dName);
	}
	Dish(double price)
	{
		setPrice(price);
	}
	
}
