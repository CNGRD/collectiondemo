package has_a_relation;
import java.util.Scanner;
class Dish
{
	private String dName;
	private double price;
	//getter
	String getDname()
	{
		return dName;
	}
	double getPrice()
	{
		return price;
	}
	//setters
	void setDname(String dName)
	{
		this.dName=dName;
	}
	void setPrice(double price)
	{
		this.price=price;
	}
	Dish(String dName)
	{
		setDname(dName);
	}
	Dish(double price)
	{
		setPrice(price);
	}
	
}
class Menu
{
	Dish[] d=new Dish[5];
}
class MenuApp {

	public static void main(String[] args) 
	{
		Scanner s=new Scanner(System.in);
		char status;
		do
		{
			System.out.println("***MENU***");
			System.out.println("Press 1 to Add Dish:");
			System.out.println("Press 2 to search Dish:");
			System.out.println("Press 3 to Get Dish:");
			int ch=s.nextInt();
			switch(ch)
			{
				case 1:
				case 2:
				case 3:
			}
			System.out.println("Want to Repeat reading Menu,Press 'Y'");
			System.out.println("Want to Stop reading Menu ,Press 'N'");
			char status=s.next().charAt(0);
		
		}while(status=='Y');
	}

}
