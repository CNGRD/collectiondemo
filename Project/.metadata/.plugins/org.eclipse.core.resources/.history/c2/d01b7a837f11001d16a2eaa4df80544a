package string_class_programs;

public class Pgm3 
{

	public static void main(String[] args) 
	{
		String s="JAVA";
		char a=s.charAt(0);
		char b=s.charAt(1);
		char c=s.charAt(2);
		char d=s.charAt(3);
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
		
	}

}
