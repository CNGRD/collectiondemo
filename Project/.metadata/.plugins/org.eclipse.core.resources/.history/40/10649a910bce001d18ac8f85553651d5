package methods;

public class Demo2 
{
	public static int num1;
	public int num2;
	public static void add()
	{
		num1=10;
		num2=20;
		System.out.println(num1+num2);
	}
	public void sub()
	{
		num1=20;
		num2=10;
		System.out.println(num1-num2);
		
	}
	public static void main(String[] args) 
	{
		Demo2 d=new Demo2();
		add();
		d.sub();
	}

}
