package com.learning_Locators;

import org.openqa.selenium.By;
import org.openqa.selenium.chrome.ChromeDriver;

public class Learning_class_Name2_Locator {

	public static void main(String[] args) throws InterruptedException {
		ChromeDriver driver=new ChromeDriver();
		driver.manage().window().maximize();
		driver.get("http://laptop-fd4nvgge/login.do");
		
		Thread.sleep(2000);
		
		driver.findElement(By.className("username")).sendKeys("admin");
		driver.findElement(By.className("pwd")).sendKeys("Manager");
		driver.findElement(By.className("initial")).click();
	}

}
